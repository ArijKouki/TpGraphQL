datasource db {
  provider = "sqlite"
  url      = "C:/Users/MSI/Desktop/tp4.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  user_id        Int     @id @default(autoincrement())
  name      String
  email     String  @unique
  role      String
  cv        Cv[]
}

model Cv {
  cv_id        Int     @id @default(autoincrement())
  name      String
  age       Int
  job       String
  user      User    @relation(fields: [user_id], references: [user_id])
  user_id    Int
  skills    Skill[] @relation("cv_skill")
}

model Skill {
  skill_id        Int     @id @default(autoincrement())
  designation String
  cvs        Cv[]      @relation("cv_skill")
}